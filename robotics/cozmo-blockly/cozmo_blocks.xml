          <value name="Y2">
            <shadow type="math_number">
              <field name="NUM">20</field>
            </shadow>
          </value>
          <value name="DEPTH">
            <shadow type="math_number">
              <field name="NUM">1</field>
            </shadow>
          </value>
          <value name="HEIGHT">
            <shadow type="math_number">
              <field name="NUM">3</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_maze">
        </block>
        <block type="cozmo_define_custom_cube">
          <field name="CUBE_DIMENSION">45</field>
          <field name="MARKER_DIMENSION">35</field>
        </block>
        <block type="cozmo_define_custom_wall">
          <field name="CUSTOM_TYPE">CustomObjectTypes.CustomType01</field>
          <field name="WALL_X_DIMENSION">90</field>
          <field name="WALL_Y_DIMENSION">120</field>
          <field name="MARKER_DIMENSION">35</field>
        </block>
        <block type="cozmo_define_custom_box">
          <field name="CUSTOM_MARKER_FACE_1">CustomObjectMarkers.Hexagons2</field>
          <field name="CUSTOM_MARKER_FACE_2">CustomObjectMarkers.Triangles2</field>
          <field name="CUSTOM_MARKER_FACE_3">CustomObjectMarkers.Triangles3</field>
          <field name="CUSTOM_MARKER_FACE_4">CustomObjectMarkers.Triangles4</field>
          <field name="CUSTOM_MARKER_FACE_5">CustomObjectMarkers.Triangles5</field>
          <field name="CUSTOM_MARKER_FACE_6">CustomObjectMarkers.Hexagons5</field>
          <field name="CUSTOM_TYPE">CustomObjectTypes.CustomType02</field>
          <field name="WALL_DEEP_DIMENSION">100</field>
          <field name="WALL_WITH_DIMENSION">20</field>
          <field name="WALL_TALL_DIMENSION">50</field>
          <field name="MARKER_DIMENSION">35</field>
