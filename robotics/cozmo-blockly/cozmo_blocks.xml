        <block type="cozmo_stop">
        </block>
      </category>
      <category name="{catCozmoActuatorsBasic}" colour="96">
        <block type="cozmo_say">
          <value name="TEXT">
            <shadow type="text">
              <field name="TEXT">{BlockCozmoSay01}</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_play_animation">
        </block>
        <block type="cozmo_play_emotion">
        </block>
        <block type="cozmo_lift">
          <value name="LIFT">
            <shadow type="math_number">
              <field name="NUM">0.0</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_head">
          <value name="HEAD">
            <shadow type="math_angle">
              <field name="NUM">0</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_drive_distance_speed">
          <value name="DISTANCE">
            <shadow type="math_number">
              <field name="NUM">10</field>
            </shadow>
          </value>
          <value name="SPEED">
            <shadow type="math_number">
              <field name="NUM">10</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_drive_wheels_speed">
          <value name="L_SPEED">
            <shadow type="math_number">
              <field name="NUM">10</field>
            </shadow>
          </value>
          <value name="R_SPEED">
            <shadow type="math_number">
              <field name="NUM">10</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_turn">
          <value name="ANGLE">
            <shadow type="math_angle">
              <field name="NUM">0</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_stop">
        </block>
        <block type="cozmo_control_cube_lights">
        </block>
        <block type="cozmo_control_backpack_lights">
        </block>
      </category>
      <category name="{catCozmoActuatorsAdvanced}" colour="96">
        <block type="cozmo_goto_origin">
        </block>
        <block type="cozmo_drive_to">
          <value name="X">
            <shadow type="math_number">
              <field name="NUM">45</field>
            </shadow>
          </value>
          <value name="Y">
            <shadow type="math_number">
              <field name="NUM">45</field>
            </shadow>
          </value>
        </block>
        <block type="cozmo_add_static_model">
          <value name="X1">
            <shadow type="math_number">
